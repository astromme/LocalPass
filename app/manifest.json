{
  "name": "LocalPass",
  "description": "A local password manager app",
  "version": "0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "fileSystem"
  ],
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}