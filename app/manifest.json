{
  "name": "LocalPass",
  "description": "A local password manager app",
  "version": "0.2.1",
  "minimum_chrome_version": "27",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "fileSystem",
    "clipboardWrite",
    "syncFileSystem"
  ],
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}