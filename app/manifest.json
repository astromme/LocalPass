{
  "name": "LocalPass Dev",
  "key": "nepghjcnamfkaafhklaenikpemdbdmdi",
  "description": "A local password manager app",
  "manifest_version": 2,
  "version": "0.2.1",
  "offline_enabled": true,
  "minimum_chrome_version": "27",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "fileSystem",
    "clipboardWrite",
    "syncFileSystem"
  ],
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}